     ]
    }
   ],
   "source": [
    "def sum_numbers(*args):\n",
    "    result = 0\n",
    "    for num in args:\n",
    "        result += int(num)\n",
    "    return result\n",
    "numbers = input(\"숫자들을 쉼표 구분하여 쓰시오\")\n",
    "\n",
    "numbers_list = numbers.split(',')\n",
    "print(sum_numbers(*numbers_list))"
   ]
  }
